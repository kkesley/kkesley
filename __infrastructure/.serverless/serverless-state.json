                  "ForwardedValues": {
                    "QueryString": true,
                    "Cookies": {
                      "Forward": "none"
                    }
                  },
                  "TargetOriginId": "S3Origin",
                  "ViewerProtocolPolicy": "redirect-to-https"
                },
                "Comment": "Distribution of front page website. Need to replaced with the actual website.",
                "DefaultRootObject": "index.html",
